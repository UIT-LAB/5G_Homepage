<!DOCTYPE html>

<head>
    <link href="https://cdn.discordapp.com/attachments/751431280519807068/800629207469785128/5g.png" rel="shortcut icon"
        type="images/x-icon">

    <style>
        .table-hover tbody tr {
            cursor: pointer;
        }

        th{
            background-color: lightgray !important;
        }
    </style>
</head>

<body>
    <header>
        <% include ../include/header %>
    </header>
    <div>
        <img src="/images/paper.jpg" class="w-100" style="height: 400px;" alt="0">
    </div>
    <div class="container">
        <div class="row mt-5 mb-5">
            <% include ../include/research_nav %>
            <div class="col-lg-12 col-xl-10">
                    <% include ../include/research_label %>
                        <div class="border border-2 p-4">
                            <h4>
                                <%=result[0].invention_name%>
                            </h4>
                            <table class="table mt-2">
                                <tbody class="align-middle">
                                    <tr>
                                        <td>
                                            번호 : <%= l_num %>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                            <div class="row mt-4">
                                <div class="col text-center">
                                    <%var str_split=result[0].application_number.split('-');%>
                                        <%var img_src="" %>
                                            <%for(var i=0; i<str_split.length; i++){ img_src=img_src+str_split[i]; }%>
                                                <%if(result[0].country_application=="KR" ){%>
                                                    <img class="img-fluid img-thumbnail mx-auto d-block m-3" width="80%"
                                                        src="/images/license/<%=img_src%>.jpg"
                                                        title="이미지를 찾을 수 없습니다."><br>
                                                    <%}%>
                                                        <table class="table mx-auto table-bordered" style="width: 80%;">
                                                            <tbody>
                                                                <tr>
                                                                    <th>사업연도</th>
                                                                    <td>
                                                                        <%=result[0].business_year%>년
                                                                    </td>
                                                                    <th>국내외구분</th>
                                                                    <td>
                                                                        <%=result[0].domestic_division.slice(3,result[0].domestic_division.langth)%>
                                                                    </td>
                                                                    <th>출원국</th>
                                                                    <td>
                                                                        <%=result[0].country_application%>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <th scope="row">출원구분</th>
                                                                    <td>
                                                                        <%=result[0].application_category.slice(3,result[0].application_category.langth)%>
                                                                    </td>
                                                                    <th>출원번호</th>
                                                                    <td colspan="3">
                                                                        <%=result[0].application_number%>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <th scope="row">출원등록기관</th>
                                                                    <td colspan="5">
                                                                        <%=result[0].application_registration_period%>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <th scope="row">출원기관<br>사업자등록번호</th>
                                                                    <td class="align-middle" colspan="5">
                                                                        <%=result[0].company_registration_number%>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <th>출원일</th>
                                                                    <td>
                                                                        <%=dayjs(result[0].application_date).format('YYYY.MM.DD');%>
                                                                    </td>
                                                                    <th>공개여부</th>
                                                                    <td>
                                                                        <%=result[0].public_not.slice(2,result[0].public_not.langth)%>
                                                                    </td>
                                                                    <th>기여율</th>
                                                                    <td>
                                                                        <%=result[0].contribution_rate%>%
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <th scope="row">발명자</th>
                                                                    <td colspan="2">
                                                                        <%=result[0].inventor%>
                                                                    </td>
                                                                    <th>소유기관</th>
                                                                    <td colspan="2">
                                                                        <%=result[0].owning_institution.slice(3,result[0].owning_institution.langth)%>
                                                                    </td>
                                                                </tr>
                                                                <tr class="align-middle">
                                                                    <th scope="row">활용형태</th>
                                                                    <td colspan="2">
                                                                        <%=result[0].application_form.slice(3,result[0].application_form.langth)%>
                                                                    </td>
                                                                    <th>표준특허여부</th>
                                                                    <td colspan="2">
                                                                        <%=result[0].standard_license_status.slice(3,result[0].standard_license_status.langth)%>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                    <button class="btn btn-outline-dark float-end" onclick="location.href='/research/license/update/<%=l_num%>'">수정</button>
                                    <form action = '/research/license/delete/<%=l_num%>?_method=PUT' method = 'POST'>
                                        <input type = 'submit' class="btn btn-outline-dark float-end" value = '삭제'>
                                    </form>
                                </div>
                            </div>
                        </div>
                </div>
        </div>
    </div>
    <footer>
        <% include ../include/footer %>
    </footer>
</body>

</html>